<Page x:Class="DVD.Pages.capch"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:DVD.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="capch">
    <Grid>
        <Grid Width="500" Height="250">
            <StackPanel x:Name="SpCatch" Orientation="Horizontal" VerticalAlignment="Center"></StackPanel>
            <Canvas x:Name="cv" />
        </Grid>
        <StackPanel>
            <Button x:Name="UpdCatch" Content="Обновить" Click="UpdCatch_Click"></Button>
            <TextBox x:Name="tbCatch"></TextBox>
            <Button x:Name="CheckCatch" Content="Проверить" Click="CheckCatch_Click" ></Button>
        </StackPanel>
    </Grid>
</Page>
